namespace LISTA02_exercicio01
{
    //1. Escreva um programa em C# que calcule a soma dos números dados pelo usuário.
    //Considere que a quantidade de números o usuário deve inserir.

    class Program
    {
        static void Main(string[] args)
        {
            int n1 = Convert.ToInt32(Console.ReadLine());
            int n2 = Convert.ToInt32(Console.ReadLine());
            int r = n1 + n2;
            Console.WriteLine("O valor de " + n1 + " + " + n2 + " é " + r)
        }
    }
}
